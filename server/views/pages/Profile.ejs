<!doctype HTML>
<html>
<head>
    <meta charset="UTF-8">
    <% include ../partials/javascript %>
    <% include ../partials/stylesheet %>
    <title><%= title %></title>
</head>
<body>
    <% include ../partials/header %>
    <div class="body">
        <img src="<%= avatar %>" alt="Profile Picture" width="350px" height="350px" style="border-radius: 50%"><br>
        <strong><%= username %></strong><br>
        <p><strong>Email</strong>: <%= email %><br></p>
        <p><strong>User ID</strong>: <%= userID %></p>
        <p><strong>Date Joined</strong>: <%= dateJoined %></p>
        <p><strong>Type (Member/Admin)</strong>: <%= type %></p>
        <p><strong>Membership Rank</strong>: <%= membership %></p>
    </div>
    <a href="/logout" class="btn btn-default">Logout</a>
    <% include ../partials/footer %>
</body>  
</html>
